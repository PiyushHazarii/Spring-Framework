<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <bean class ="me.beans.Student" id="std1">
    	<property name = "name" value = "Piyush"/>
    	<property name = "rollno" value = "95"/>
    </bean>
    
		<!--we can get the object throught name also  -->
    <bean class ="me.beans.Student" name="std2">
    	<property name = "name" value = "Sanju"/>
    	<property name = "rollno" value = "59"/>
    </bean>

</beans>
<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <bean class = "me.beans.Student" id ="std">
	    <property name= "name" value="Piyush"/>
	    <property name = "roll" value = "98"/>
	    <property name = "subjectList">
	    	<list>
	    		<value>C</value>
	    		<value>C++</value>
	    		<value>Java</value>
	    		<value>Python</value>
	    		<value>SQL</value>
	    	</list>
	    </property>
	    <property name = "map">
	    	<map>
	    		<entry key="Piyush" value="90"/>
	    		<entry key="Aman" value="93"/>
	    		<entry key="Abhi" value="88"/>
	    		<entry key="Rahul" value="38"/>
	    	</map>
	    </property>
    </bean>

</beans>